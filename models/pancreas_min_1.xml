      <parameter id="Cext_ins" name="insulin concentration (Vext)" units="mM" constant="false"/>
      <parameter id="Cext_cpep" name="c-peptide concentration (Vext)" units="mM" constant="false"/>
      <parameter id="Cpa_glc" name="glucose concentration (Vpa)" units="mM" constant="false"/>
      <parameter id="Cpa_lac" name="lactate concentration (Vpa)" units="mM" constant="false"/>
      <parameter id="GLCIM_Vmax" name="Glucose import" value="100" units="mmole_per_minl" constant="true"/>
      <parameter id="GLCIM_Km" value="1" units="mM" constant="true"/>
      <parameter id="LACEX_Vmax" name="Lactate import" value="100" units="mmole_per_minl" constant="true"/>
