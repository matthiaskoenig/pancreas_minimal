    </listOfRules>
    <listOfReactions>
      <reaction sboTerm="SBO:0000655" id="GLCIM" name="glucose import" reversible="true" fast="false" compartment="Vpa">
        <listOfReactants>
          <speciesReference species="Aext_glc" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Apa_glc" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:sbml="http://www.sbml.org/sbml/level3/version1/core">
            <apply>
              <divide/>
              <apply>
                <times/>
                <apply>
                  <divide/>
                  <apply>
                    <times/>
                    <ci> Vpa </ci>
                    <ci> GLCIM_Vmax </ci>
                  </apply>
                  <ci> GLCIM_Km </ci>
                </apply>
                <apply>
                  <minus/>
                  <ci> Cext_glc </ci>
                  <ci> Cpa_glc </ci>
                </apply>
              </apply>
              <apply>
                <plus/>
                <cn sbml:units="dimensionless" type="integer"> 1 </cn>
                <apply>
                  <divide/>
                  <ci> Cext_glc </ci>
                  <ci> GLCIM_Km </ci>
                </apply>
                <apply>
                  <divide/>
                  <ci> Cpa_glc </ci>
                  <ci> GLCIM_Km </ci>
                </apply>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction sboTerm="SBO:0000655" id="LACEX" name="lactate export" reversible="true" fast="false" compartment="Vpa">
        <listOfReactants>
          <speciesReference species="Apa_lac" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Aext_lac" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:sbml="http://www.sbml.org/sbml/level3/version1/core">
            <apply>
              <divide/>
              <apply>
                <times/>
                <apply>
                  <divide/>
                  <apply>
                    <times/>
                    <ci> Vpa </ci>
                    <ci> LACEX_Vmax </ci>
                  </apply>
                  <ci> LACEX_Km </ci>
                </apply>
                <apply>
                  <minus/>
                  <ci> Cpa_lac </ci>
                  <ci> Cext_lac </ci>
                </apply>
              </apply>
              <apply>
                <plus/>
                <cn sbml:units="dimensionless" type="integer"> 1 </cn>
                <apply>
                  <divide/>
                  <ci> Cext_lac </ci>
                  <ci> LACEX_Km </ci>
                </apply>
                <apply>
                  <divide/>
                  <ci> Cpa_lac </ci>
                  <ci> LACEX_Km </ci>
                </apply>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction sboTerm="SBO:0000176" id="GLC2LAC" name="glycolysis" reversible="false" fast="false" compartment="Vpa">
        <listOfReactants>
          <speciesReference species="Apa_glc" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Apa_lac" stoichiometry="2" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> Vpa </ci>
              <ci> GLC2LAC_Vmax </ci>
              <apply>
                <divide/>
                <ci> Cpa_glc </ci>
                <apply>
                  <plus/>
                  <ci> Cpa_glc </ci>
                  <ci> GLC2LAC_Km </ci>
                </apply>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="IRS" name="IRS insulin secretion" reversible="false" fast="false" compartment="Vpa">
        <listOfProducts>
          <speciesReference species="Aext_ins" stoichiometry="1" constant="true"/>
          <speciesReference species="Aext_cpep" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <listOfModifiers>
          <modifierSpeciesReference species="Apa_glc"/>
        </listOfModifiers>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> Vpa </ci>
                <ci> IRS_Vmax </ci>
                <apply>
                  <power/>
                  <ci> Cpa_glc </ci>
                  <ci> IRS_n_glc </ci>
                </apply>
              </apply>
              <apply>
                <plus/>
                <apply>
                  <power/>
                  <ci> Cpa_glc </ci>
                  <ci> IRS_n_glc </ci>
                </apply>
                <apply>
                  <power/>
                  <ci> IRS_Km_glc </ci>
                  <ci> IRS_n_glc </ci>
                </apply>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
