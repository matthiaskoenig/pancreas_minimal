      <parameter id="IRS_Km_glc" value="7" units="mM" constant="true"/>
    </listOfParameters>
    <listOfRules>
      <assignmentRule variable="Cext_glc">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
